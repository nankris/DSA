class Solution:
    def kClosest(self, points: List[List[int]], k: int) -> List[List[int]]:

        heap_ = []
        heapq.heapify(heap_)
        for point in points:
            dist = (point[0] - 0)**2 + (point[1] - 0)**2
            heapq.heappush(heap_, (-dist, point))
            if len(heap_) > k:
                heapq.heappop(heap_)
        fin_lst = []
        for dist, pnt in heap_:        
            fin_lst.append(pnt)

        return fin_lst
