class Solution:
    def minCostClimbingStairs(self, cost: List[int]) -> int:
        dp = [0]*len(cost)
        print(dp)

        dp[len(cost)-1] = cost[-1]
        # This is for top we are assigning 0
        dp.append(0)

        for i in range(len(cost)-2, -1, -1):
            dp[i] = cost[i] + min(dp[i+1], dp[i+2])
        

        return min(dp[0], dp[1])
